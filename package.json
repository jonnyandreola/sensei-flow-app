{
  "name": "svelte-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "rollup -c",
    "graphql-start": "openapi-to-graphql http://localhost:62609/swagger/v1/swagger.json --url http://localhost:62609 --cors",
    "dev": "rollup -c -w",
    "start": "sirv public",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "test:coverage": "node -e \"console.log('❌ -> Coverage is currently not working, please check status here: https://github.com/mihar-22/svelte-jester/issues/33 and then replace this with `npm test -- --collectCoverage`');\"",
    "validate": "svelte-check",
    "lint": "node_modules/eslint/bin/eslint.js ./src",
    "generate-schema": "npx openapi-typescript ./swagger.json -o ./scr/types/generated-schema.d.ts"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.13.12",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@rollup/plugin-typescript": "^8.0.0",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/svelte": "^3.0.3",
    "@tsconfig/svelte": "^1.0.0",
    "@types/jest": "^26.0.21",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.22.0",
    "eslint-plugin-svelte3": "^3.1.2",
    "jest": "^26.6.3",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.0.0",
    "svelte-check": "^1.0.0",
    "svelte-jester": "^1.3.1",
    "svelte-preprocess": "^4.0.0",
    "ts-jest": "^26.5.4",
    "tslib": "^2.0.0",
    "typescript": "^4.0.0"
  },
  "dependencies": {
    "dialog-polyfill": "^0.5.6",
    "graphql": "^15.5.0",
    "graphql-request": "^3.4.0",
    "openapi-to-graphql": "^2.3.0",
    "sirv-cli": "^1.0.0",
    "svelte-spa-router": "^3.1.0"
  }
}
